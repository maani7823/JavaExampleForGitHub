<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="Practice1.Design1Controller">
   <children>
      <Button fx:id="goNextSceneButton" layoutX="504.0" layoutY="343.0" mnemonicParsing="false" onAction="#goToNextScene" text="Go Next" />
      <VBox layoutX="24.0" layoutY="14.0" prefHeight="200.0" prefWidth="435.0" spacing="30.0">
         <children>
            <Label fx:id="view1Title" prefHeight="56.0" prefWidth="231.0" text="Person Details">
               <font>
                  <Font name="System Bold Italic" size="34.0" />
               </font>
            </Label>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <TextField fx:id="nameTextField" />
                  <TextField fx:id="AgeTextField" alignment="CENTER" />
                  <ColorPicker fx:id="colorPicker" prefHeight="25.0" prefWidth="141.0" />
               </children>
               <padding>
                  <Insets top="30.0" />
               </padding>
            </HBox>
            <Button fx:id="submitButton" mnemonicParsing="false" onAction="#savePersonObject" prefHeight="25.0" prefWidth="465.0" text="Button" />
         </children>
      </VBox>
   </children>
</AnchorPane>
